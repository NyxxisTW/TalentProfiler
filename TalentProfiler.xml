									<Anchor point="LEFT">
										<Offset x="12" y="0"/>
									</Anchor>
								</Anchors>
								<Layers>
									<Layer level="ARTWORK">
										<FontString name="$parent_Text" text="SYNC" font="TP_FontButton">
											<Size x="48" y="24"/>
											<Anchors>
												<Anchor point="CENTER"/>
											</Anchors>
											<Color r="1" g="1" b="1" a="1"/>
										</FontString>
									</Layer>
									<Layer level="BORDER">
										<Texture>
											<Size x="48" y="24"/>
											<Anchors>
												<Anchor point="CENTER"/>
											</Anchors>
											<Color r="0.15" g="0.15" b="0.15" a="1"/>
