									<TileSize val="8"/>
									<BackgroundInsets left="3" right="3" top="6" bottom="6"/>
								</Backdrop>
								<ThumbTexture file="Interface\Buttons\UI-SliderBar-Button-Horizontal">
									<Size x="32" y="32"/>
								</ThumbTexture>
								<Scripts>
									<OnValueChanged>
										TP_Profiles_ScrollFrame:SetVerticalScroll(this:GetValue())
									</OnValueChanged>
									<OnShow>
										this:SetValueStep(0)
										this:SetValue(0)
									</OnShow>
								</Scripts>
							</Slider>
						</Frames>
						<Layers>
							<Layer level="BACKGROUND">
